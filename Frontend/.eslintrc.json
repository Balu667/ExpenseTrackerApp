{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "standard","prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react","prettier"],
	"rules": {
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
	"no-console": "error",
    "semi": 0,
	"prefer-const":0,
	"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
